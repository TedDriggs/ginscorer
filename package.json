{
    "name": "ginscorer",
    "version": "0.1.0",
    "private": true,
    "license": "MIT",
    "homepage": "https://teddriggs.github.io/ginscorer",
    "dependencies": {
        "classnames": "^2.3.1",
        "gh-pages": "2.0.0",
        "lodash-es": "^4.17.21",
        "memoize-one": "5.2.1",
        "node-sass-chokidar": "^1.5.0",
        "npm-run-all": "^4.1.5",
        "react": "^17.0.2",
        "react-dom": "^17.0.2",
        "react-redux": "7.2.5",
        "react-responsive": "^8.0.3",
        "react-scripts-ts": "^4.0.8",
        "react-transition-group": "^4.4.2",
        "redux": "4.1.1",
        "redux-loop": "6.1.0",
        "reselect": "3.0.1",
        "w3c-keys": "^1.0.3"
    },
    "scripts": {
        "predeploy": "npm run build",
        "deploy": "gh-pages -d build",
        "build-css": "./node_modules/node-sass-chokidar/bin/node-sass-chokidar src/ -o src/",
        "watch-css": "npm run build-css && ./node_modules/node-sass-chokidar/bin/node-sass-chokidar src/ -o src/ --watch --recursive",
        "start-ts": "react-scripts-ts start",
        "build-ts": "react-scripts-ts build",
        "start": "./node_modules/npm-run-all/bin/npm-run-all/index.js -p watch-css start-ts",
        "build": "./node_modules/npm-run-all/bin/npm-run-all/index.js build-css build-ts",
        "test": "react-scripts-ts test --env=jsdom",
        "eject": "react-scripts-ts eject",
        "format": "./node_modules/.bin/prettier -u -w --ignore-path=.gitignore"
    },
    "devDependencies": {
        "@types/classnames": "2.2.6",
        "@types/jest": "^23.3.14",
        "@types/lodash-es": "4.17.5",
        "@types/memoize-one": "5.1.2",
        "@types/node": "^10.14.6",
        "@types/react": "^17.0.24",
        "@types/react-dom": "^17.0.9",
        "@types/react-redux": "7.1.18",
        "@types/react-responsive": "8.0.3",
        "@types/react-transition-group": "4.4.3",
        "prettier": "^2.4.1",
        "typescript": "^4.4.3"
    },
    "browserslist": [
        ">0.2%",
        "not dead",
        "not ie <= 11",
        "not op_mini all"
    ]
}
